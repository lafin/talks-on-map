{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,

    "block-scoped-var": 0,
    "padded-blocks": 0,

    "no-param-reassign": 0,
    "func-names": 0,
    "no-shadow": 0,
    "no-console": 0,
    "strict": 0,
    "no-else-return": 0
  },
  "plugins": [
    "react"
  ]
}
